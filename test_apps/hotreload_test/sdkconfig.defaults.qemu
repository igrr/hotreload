# QEMU-specific configuration
# This file is applied when building for QEMU emulation

# Network configuration for QEMU (OpenETH virtual NIC)
CONFIG_EXAMPLE_CONNECT_WIFI=n
CONFIG_EXAMPLE_CONNECT_ETHERNET=y
CONFIG_ETH_USE_OPENETH=y
CONFIG_ETHERNET_OPENETH_SUPPORT=y

# Disable internal EMAC and other Ethernet drivers (not supported in QEMU)
CONFIG_ETHERNET_INTERNAL_SUPPORT=n
